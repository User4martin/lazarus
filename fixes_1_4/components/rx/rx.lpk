        <UnitName Value="MRUList"/>
      </Item2>
      <Item3>
        <Filename Value="placement.pp"/>
        <UnitName Value="Placement"/>
      </Item3>
      <Item4>
        <Filename Value="strholder.pp"/>
        <HasRegisterProc Value="True"/>
        <UnitName Value="StrHolder"/>
      </Item4>
