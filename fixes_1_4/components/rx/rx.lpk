<?xml version="1.0"?>
<CONFIG>
  <Package Version="3">
    <Name Value="rx"/>
    <Author Value="Michael Van Canneyt, AO ROSNO, Master-Bank"/>
    <CompilerOptions>
      <Version Value="8"/>
