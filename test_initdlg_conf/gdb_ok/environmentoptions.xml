<?xml version="1.0"?>
<CONFIG>
  <EnvironmentOptions>
    <Version Value="110" Lazarus="2.3.0"/>
    <LazarusDirectory Value="%LazDir%">
    </LazarusDirectory>
    <CompilerFilename Value="%FpcBinDir%\fpc.exe">
    </CompilerFilename>
    <FPCSourceDirectory Value="$(LazarusDir)\fpc\$(FPCVer)\source">
    </FPCSourceDirectory>
    <MakeFilename Value="%FpcBinDir%\make.exe">
    </MakeFilename>
    <TestBuildDirectory Value="%Temp%">
    </TestBuildDirectory>
    <Debugger>
      <Configs>
        <Config ConfigName="lldb" ConfigClass="TLldbDebugger" DebuggerFilename="$(LazarusDir)\lazarus.exe" />
        <Config ConfigName="gdb" ConfigClass="TGDBMIDebugger" DebuggerFilename="$(LazarusDir)\lazarus.exe" Active="True"/>
        <Config ConfigName="FpDebug" ConfigClass="TFpDebugDebugger" />
      </Configs>
    </Debugger>
  </EnvironmentOptions>
</CONFIG>
